@model Nest.ISearchResponse<Book>

@if (Model.Documents.Any())
{
    <h3>Search Results</h3>
    <ul>
        @foreach (var book in Model.Documents)
        {
            <li>Title: @book.Title (ISBN: @book.Isbn)</li>
        }
    </ul>
}
else
{
    <h3>No matches found :(</h3>
}